<?xml version="1.0" encoding="utf-8"?>
<openerp><data>

    <record id="view_web_query_timeout_tree" model="ir.ui.view">
        <field name="name">web.query.timeout.tree</field>
        <field name="model">web.query.timeout</field>
        <field name="arch" type="xml">
            <tree string="Query time outs" create="false">
