                    <field name="create_uid"/>
                </group>
            </form>
        </field>
    </record>

    <record id="action_web_query_timeout" model="ir.actions.act_window">
        <field name="name">UI Query time outs</field>
        <field name="res_model">web.query.timeout</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
